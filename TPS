local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()

local Window = Rayfield:CreateWindow({
   Name = "HellScripts",
   LoadingTitle = "HellHub",
   LoadingSubtitle = "HellHub",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil, -- Create a custom folder for your hub/game
      FileName = "HellHub"
   },
   Discord = {
      Enabled = false,
      Invite = "https://discord.gg/xfMen4HU", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ABCD would be ABCD
      RememberJoins = true -- Set this to false to make them join the discord every time they load it up
   },
   KeySystem = false, -- Set this to true to use our key system
   KeySettings = {
      Title = "HellHub",
      Subtitle = "Key System",
      Note = "https://discord.gg/xfMen4HU",
      FileName = "Key", -- It is recommended to use something unique as other scripts using Rayfield may overwrite your key file
      SaveKey = true, -- The user's key will be saved, but if you change the key, they will be unable to use your script
      GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
      Key = {"HellScripts1984"} -- List of keys that will be accepted by the system, can be RAW file links (pastebin, github etc) or simple strings ("HellScripts1984","key22")
   }
})

local Tab = Window:CreateTab("TPS", 4483362458) -- Title, Image

local Section = Tab:CreateSection("script")

local Button = Tab:CreateButton({
   Name = "tps street soccer",
   Callback = function()
   loadstring(game:HttpGet('https://raw.githubusercontent.com/hussain1323232234/My-Scripts/main/Street%20Soccer'))()
   end,
})
